{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:recommended",
		":semanticCommitTypeAll(chore)"
	],
	"rangeStrategy": "bump",
	"packageRules": [
		{
			"matchManagers": [
				"npm"
			],
			"matchDepTypes": [
				"devDependencies"
			],
			"enabled": false
		}
	]
}
